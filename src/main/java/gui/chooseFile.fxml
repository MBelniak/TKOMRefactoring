<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ChooseFileController">
    <AnchorPane layoutY="7.0" prefHeight="400.0" prefWidth="600.0">
        <Pane layoutX="200.0" layoutY="100.0" prefHeight="220.0" prefWidth="200.0">
            <ComboBox fx:id="fileComboBox" layoutX="25.0" layoutY="14.0" onAction="#sourceFileChanged" prefWidth="150.0" />
            <Button fx:id="nextButton" layoutX="62.0" layoutY="171.0" mnemonicParsing="false" onAction="#nextButtonClicked" text="Refactor" />
         <ComboBox fx:id="classesComboBox" layoutX="25.0" layoutY="66.0" onAction="#sourceClassChanged" prefWidth="150.0" />
         <ComboBox fx:id="statementsComboBox" layoutX="25.0" layoutY="116.0" onAction="#statementChanged" prefWidth="150.0" />
        </Pane>
    </AnchorPane>
</AnchorPane>
